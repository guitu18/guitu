package excel;

import cn.afterturn.easypoi.excel.annotation.Excel;
import lombok.Data;

/**
 * @author zhangkuan
 * @date 2019/8/21
 */
@Data
public class Student {
    @Excel(name = "学生姓名")
    private String name;
    @Excel(name = "年龄")
    private Integer age;

    public Student() {
    }

    public Student(String name, Integer age) {
        this.name = name;
        this.age = age;
    }
}
